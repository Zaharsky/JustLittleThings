class Human:
    """
    Utwórz klasę Human reprezentującą człowieka,
    musi posiadać atrybuty takie jak wiek, waga, wzrost, imię i płeć.
    Klasa powinna także zawierać metody getAge, getWeight, getHeight, getName, isMale.
     """
    def __init__(self, age,weight,height,name,sex):
        self.age=age
        self.weight=weight
        self.height=height
        self.name=name
        self.sex=sex

    def getAge(self):
        print(self.age)

    def getWeight(self):
        print(self.weight)

    def getHeight(self):
        print(self.height)

    def getName(self):
        print(self.name)

    def isMale(self):
        if self.sex == "M":
            print("yes")
        else:
            print("not male")


human1 = Human(20,85,180,"Dawid","M")
print(Human.__doc__)
# Human.getAge(human1)
human1.isMale()
